<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>

<%- include('../partials/Modal'); %>

<body class="container">

    <header>
        <%- include('../partials/header'); %>
    </header>

    <main class="container">
        <div class="jumbotron container">
            <h2 class="text-center">Updating Selected Service</h2>
            <form action="/chemical/update" method="post">
              <div>
                  <input class="form-control" name="chemical_list" id="service_id" type="hidden" value=<%=chemical_list %>>
                  <label>Chemical Name</label>
                    <select class="form-control" name="state" id="state">
                        <option value=<%=state%>><%=state%></option>
                        <% states.forEach(function(states) { %>              
                        <option value=<%=states.state_code_id %>><%=states.state_name %></option>
                        <% }); %>
                    </select>
                  <label>Supplier</label>
                    <select class="form-control" name="state" id="state">
                        <option value=<%=state%>><%=state%></option>
                        <% states.forEach(function(states) { %>              
                        <option value=<%=states.state_code_id %>><%=states.state_name %></option>
                        <% }); %>
                    </select>
                    <label>License</label>
                    <select class="form-control" name="state" id="state">
                        <option value=<%=state%>><%=state%></option>
                        <% states.forEach(function(states) { %>              
                        <option value=<%=states.state_code_id %>><%=states.state_name %></option>
                        <% }); %>
                    </select>
                  <label>Chemical Name</label>
                      <input class="form-control" name="chemcial_name" value=<%=chemcial_name%>>
                  <label>Chemical Name</label>
                    <input class="form-control" name="chemcial_type" value=<%=chemcial_type%>>
                  <button type="submit" style="background-color:#198754" class="btn btn-success">Submit</button>
              </div>
            </form> 
        </div>
    </main>

    <footer>
        <%- include('../partials/footer'); %>
    </footer>

</body>
</html>